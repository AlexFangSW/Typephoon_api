{
  "db": {
    "host": "localhost",
    "port": 5432,
    "db": "typephoon",
    "username": "typephoon",
    "password": "123",
    "echo": true
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "db": 0
